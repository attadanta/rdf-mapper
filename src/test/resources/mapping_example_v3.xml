<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<mapping xmlns="http://imi.kit.edu/">
	<namespace-map>
		<ns>
			<label><!-- the prefix of the namespace --></label>
			<uri></uri>
		</ns>
		<!--  ...  -->
	</namespace-map>
    
    <class-map>
        <class-entity>
            <label><!-- name without url encoding --></label>
            <uri><!-- can contain replaceable parts like: $idf-index$ --></uri>
            <entity-name></entity-name>
            <class-uri></class-uri>
            <!--  ...  -->
            <property-map>
                <class-property>
                    <label><!-- name without uri encoding --></label>
                    <uri><!-- property uri with encoding --></uri>
                    <identifier><!-- the property to map name in class (e.g. N1) or index in idf individual --></identifier>
                    <property-type><!-- object-property or data-property --></property-type>
                    <datatype><!-- e.g. "object-uri" or uri for the datatype (http://www.w3.org/2001/XMLSchema#string)--></datatype>
                    <object-uri><!-- object-uri (used only for object properties --></object-uri>
                </class-property>
                <!--  ...  -->
            </property-map>
        </class-entity>
        <!--  ...  -->
    </class-map>
    <taxonomy-map>
        <subclass-relation>
            <super-class></super-class>
            <sub-class></sub-class>
        </subclass-relation>
        <!--  ...  -->
    </taxonomy-map>
</mapping>